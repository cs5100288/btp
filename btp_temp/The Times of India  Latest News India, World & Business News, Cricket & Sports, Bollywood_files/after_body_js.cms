						{
							if(document.getElementById("newsbrk").style.display == "inline")
							{
								document.getElementById("newsbrk").style.display = "none";
							}
							setTimeout("bnews( )",12000);
						}
						else
						{
							if($.fn.webTicker)
							{
							insertnews(bstr);
							}
							else
							{
attachcss("/default_webticker_cssv2.cms?version=2");                             
$.get("/default_webticker_jsone_v2.cms?version=2",function(datab){
                             eval(datab);
                               	insertnews(bstr);
                              });

							}
						}
					}
				}
			}
			breakhttp.open("GET",'/breakingnews/breakingnews.html',true);
			breakhttp.send(null);
		}
	}
	catch (e){breakhttp = false;}
}

